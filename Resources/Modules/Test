class A {
	var a: Int = -1,
		b: (Int) -> ()

	init(/*_*/ b: /*@escaping*/ (Int) -> ()) {
		self.b = b
		self.b(0)
	}
}

class B {
	var a: Int = -1,
		b: (Int) -> ()

	init(/*_*/ b: /*@escaping*/ (Int) -> ()) {
		self.b = b
		self.b(1)
	}
}

struct C {
	static var a: Int = 0

	static func e(n: Int) -> () {
		self.a = n
	}
}

var a = A(C.e),
	b = B(C.e)

print(a.a == -1)
print(b.a == -1)
print(C.a == 1)

return

