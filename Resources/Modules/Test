var a: [Global]

func A() {
	struct AB {}  // Retain chain: AB -> Call<A> -> A -> Global

	return AB
}

func B() {
	A()

	return A()
}

func C(CC) {
	return CC
}

var retainTest = C(CC: B())  // Preserve value globally, simple return cleans up memory

return retainTest

