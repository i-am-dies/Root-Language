class Base {
	/*virtual*/ var a = 'Base'

	init {
		f()
		print(super)
		print(self)
		print(sub)
	}

	/*virtual*/ func f() {
		print(a)
	}
}

class Derived: Base {
	var a = 'Derived'

	init {
		super()
		f()
		print(super)
		print(self)
		print(sub)
	}

	func f() {
		print(a)
	}
}

var d = Derived()

return d

