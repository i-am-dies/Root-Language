	when color {
		.RED -> "кот"
		.ORANGE -> "охотник"
		.YELLOW -> "желает"
		.GREEN -> "знать"
		.BLUE -> "где"
		.INDIGO -> "сидят"
		.VIOLET -> "фазаны"
	}

func getWarmth(color: Int) {
	when color {
		RED || YELLOW: 'warm'
		GREEN: 'neutral'
		BLUE: 'cold'
		else: 'unknown'
	}
}
